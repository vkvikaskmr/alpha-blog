<h1>Showing Articles Index page</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
    </tr>
  </thead>
  <% @articles.each do |article|%>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.description %></td>
      <td><%= link_to "Show", article_path(article) %></td>
    </tr>
  <% end %>
</table>
